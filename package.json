{
  "name": "video-player-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "flow-bin": "^0.72.0",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-localization": "^1.0.10",
    "react-md": "^1.3.1",
